generator client {
  provider = "prisma-client-js"
  engineType = "binary"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model GasCache {
  id       Int      @id @default(autoincrement())
  address  String
  dateStr  String
  txCount  Int
  feeUsdt  String
  feeRmb   String
  updatedAt DateTime @updatedAt

  @@unique([address, dateStr])
  @@map("GasCache")
}
